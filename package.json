{
  "name": "AcademyCoreAPI",
  "version": "0.1.0",
  "description": "API for management of students from a university or academy",
  "main": "dist/index.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/LZJorge/AcademyCoreAPI.git"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/main.js",
    "build": "tsc && tsc-alias",
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/main.ts",
    "test": "cross-env NODE_ENV=test jest --no-cache --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=test jest --watch --no-cache --verbose --detectOpenHandles",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Jorge Landaeta <dev.jorge2003@gmail.com>",
  "license": "GPL-3.0-only",
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.2.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/jest": "^29.5.11",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-plugin-jest": "^27.6.3",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "jest-mock-extended": "^3.0.5",
    "prisma": "^5.4.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "bcrypt": "^5.1.0",
    "chalk": "4.1.2",
